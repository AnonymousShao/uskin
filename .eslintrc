{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jasmine": true
  },
  "globals": {
    "uskin": false,
    "React": false,
    "ReactDOM": false
  },
  "plugins": [
    "react",
    "babel"
  ],
  "rules": {
    "constructor-super": 2,
    "comma-dangle": [2, "never"],
    "eol-last": 2,
    "func-names": 0,
    "guard-for-in": 0,
    "key-spacing": 0,
    "no-eq-null": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-var": 0,
    "no-unused-expressions": 0,
    "no-shadow": [2, {"allow": ["xhr"]}],
    "object-shorthand": 0,
    "one-var": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-template": 0,
    "padded-blocks": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/prop-types": [2, { "ignore": [ "children", "className", "style" ] }],
    "react/sort-comp": 0,
    "react/wrap-multilines": 0,
    "react/no-multi-comp": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "spaced-comment": 0,
    "vars-on-top": 0
  }
}
